{
  "name": "crud-app-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "husky:pre-commit": "concurrently npm:prettier npm:lint npm:stylelint npm:test:coverage",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx}' --quiet",
    "lint:fix": "npx eslint 'src/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prepare": "husky install",
    "prettier": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --check",
    "prettier:fix": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "release": "standard-version --skip.tag",
    "release:minor": "standard-version --release-as minor --skip.tag",
    "release:patch": "standard-version --release-as patch --skip.tag",
    "release:major": "standard-version --release-as major --skip.tag",
    "start": "ng serve",
    "stylelint": "npx stylelint 'src/**/*.scss' --quiet",
    "stylelint:fix": "npx stylelint 'src/**/*.scss' --quiet --fix",
    "test:coverage": "ng test --sourceMap=true --codeCoverage=true",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "concurrently": "^7.4.0",
    "ems-angular-toolkit": "^0.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "~4.6.2"
  }
}
